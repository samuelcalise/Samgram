{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": {
      "type": "object",
      "properties": {
        "FavoriteColor": {
          "type": "string"
        },
        "PetName": {
          "type": "string"
        },
        "UserBirthday": {
          "type": "string",
          "pattern": "^(0[1-9]|1[0-2])/(0[1-9]|[12][0-9]|3[01])/(19|20)\\d{2}$"
        },
        "image": {
          "type": "string",
          "format": "uri"
        },
        "name": {
          "type": "string"
        }
      },
      "required": ["FavoriteColor", "PetName", "UserBirthday", "name"],
      "additionalProperties": false
    }
  }
